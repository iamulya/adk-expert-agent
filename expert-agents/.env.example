# Google Cloud Project ID (for Secret Manager)
GCP_PROJECT_ID="your-gcp-project-id"

# Secret ID in Google Cloud Secret Manager for Gemini API Key
GEMINI_API_KEY_SECRET_ID="your-gemini-api-key-secret-id"

# Optional: Secret Version for Gemini API Key (defaults to '1' or 'latest' depending on implementation)
# GEMINI_API_KEY_SECRET_VERSION="latest"

# Secret ID in Google Cloud Secret Manager for GitHub Personal Access Token (PAT)
# This PAT is used by GetGithubIssueDescriptionTool to fetch issue details via GitHub API.
# It needs appropriate permissions (e.g., read access to public repositories).
GITHUB_API_PAT_SECRET_ID="your-github-pat-secret-id"

# Optional: Secret Version for GitHub PAT (defaults to '1')
# GITHUB_API_PAT_SECRET_VERSION="1"

# Optional: browser-use specific logging level (info, debug, result)
# BROWSER_USE_LOGGING_LEVEL="info"